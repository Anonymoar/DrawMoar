<UserControl x:Class="DrawMoar.FrameControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DrawMoar"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300"
             AllowDrop="True"
             MouseLeftButtonDown="FrameControl_MouseLeftButtonDown"
             MouseLeftButtonUp="FrameControl_MouseLeftButtonUp"
             MouseMove="FrameControl_MouseMove"
             Focusable="True"
             GotFocus="UserControl_GotFocus">
    <Grid>
        <local:VisualHost x:Name="VisualHost" />
    </Grid>
</UserControl>
